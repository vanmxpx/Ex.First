<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="SecuredByMessageEndpoint">
                    <security>
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
                <binding name="SecuredByTransporWithMessageEnpoint">
                    <security mode="TransportWithMessageCredential">
                        <transport clientCredentialType="None" />
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
                <binding name="SecuredByTransportEndpoint">
                    <security mode="Transport">
                        <transport clientCredentialType="Basic" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://127.0.0.1:8080/message" binding="wsHttpBinding"
                bindingConfiguration="SecuredByMessageEndpoint" contract="ServiceReference1.IContract"
                name="SecuredByMessageEndpoint">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
            <endpoint address="https://127.0.0.1:4000/" binding="wsHttpBinding"
                bindingConfiguration="SecuredByTransporWithMessageEnpoint"
                contract="ServiceReference1.IContract" name="SecuredByTransporWithMessageEnpoint" />
            <endpoint address="https://127.0.0.1:4000/transport" binding="wsHttpBinding"
                bindingConfiguration="SecuredByTransportEndpoint" contract="ServiceReference1.IContract"
                name="SecuredByTransportEndpoint" />
        </client>
    </system.serviceModel>
</configuration>