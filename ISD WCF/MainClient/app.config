<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="WSHttpBinding_Message">
                    <security>
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
                <binding name="WSHttpBinding_TransportWithMessage">
                    <security mode="TransportWithMessageCredential">
                        <transport clientCredentialType="None" />
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
                <binding name="WSHttpBinding_Transport">
                    <security mode="Transport">
                        <transport clientCredentialType="Basic" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/MainService/Message"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_Message"
                contract="Service.IMainService" name="WSHttpBinding_Message" 
                behaviorConfiguration="ClientCertificateBehavior">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
            <endpoint address="https://nickservice:8088/MainService/TransportWithMessage"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_TransportWithMessage"
                contract="Service.IMainService" name="WSHttpBinding_TransportWithMessage">
                <identity>
                    <dns value="nickservice" />
                </identity>
            </endpoint>
            <endpoint address="https://nickservice:8088/MainService/Transport"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_Transport"
                contract="Service.IMainService" name="WSHttpBinding_Transport">
                <identity>
                    <dns value="nickservice" />
                </identity>
            </endpoint>
        </client>
    <behaviors>
    <endpointBehaviors>
        <behavior name="ClientCertificateBehavior">
            <clientCredentials>
                <serviceCertificate>
                    <authentication certificateValidationMode="None" />
                </serviceCertificate>
            </clientCredentials>
        </behavior>
    </endpointBehaviors>
</behaviors>
    </system.serviceModel>
</configuration>